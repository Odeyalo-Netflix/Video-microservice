FROM maven:3.8.5-jdk-11-slim

COPY . /app

WORKDIR /app

CMD mvn -s maven-settings.xml -U install -DSPRING_DATASOURCE_URL=${SPRING_DATASOURCE_URL} -DSPRING_DATASOURCE_USERNAME=${SPRING_DATASOURCE_USERNAME} -DSPRING_DATASOURCE_PASSWORD=${SPRING_DATASOURCE_PASSWORD} -DSPRING_JPA_HIBERNATE_DDL_AUTO=${SPRING_JPA_HIBERNATE_DDL_AUTO} -DAPP_HOST=${APP_HOST} -DTHUMBSNAP_API_KEY=${THUMBSNAP_API_KEY} -DVIDEO_SAVING_PATH=${IMAGE_SAVING_PATH} -DIMAGE_SAVING_PATH=${IMAGE_SAVING_PATH} -DVIDEO_SAVING_PATH_TEMP=${VIDEO_SAVING_PATH_TEMP}


